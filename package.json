{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Landing page for trillo",
  "main": "index.js",
  "scripts": {
    "watch-css": "sass sass/main.scss css/style.css -w",
    "dev-server": "live-server",
    "compile-css": "sass sass/main.scss css/style.comp.css",
    "prefix-css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress-css": "sass css/style.prefix.css css/style.css --style compressed",
    "build-css": "npm-run-all compile-css prefix-css compress-css",
    "start": "npm-run-all --parallel dev-server watch-css"
  },
  "author": "Pratik Agarwalla",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.8"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  }
}
